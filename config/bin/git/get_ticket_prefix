#!/usr/bin/env bash

current_directory="${PWD#"$HOME"/}"

PS3="Select a prefix: "
case "$current_directory" in
  "Code/ultimate-tic-tac-toe")
      mapped_string="UTTT"
      ;;
  "Code/redwood")
      mapped_string="RW"
      ;;
  "Code/suralink")
      mapped_string="APP"
      ;;
  *)
      mapped_string="Unknown directory"
      ;;
esac

echo "$mapped_string"
